{
	"name": "@quikcess/sdk-bet",
	"type": "module",
	"private": false,
	"version": "0.0.8",
	"description": "Official Quikcess Bet SDK for NodeJS",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"module": "lib/index.mjs",
	"homepage": "https://github.com/quikcess/sdk-bet#readme",
	"scripts": {
		"release": "npm run lint && npm run build && changeset publish",
		"build": "tsup ./src",
		"check": "tsc --noEmit",
		"lint": "biome check --write .",
		"format": "biome format .",
		"test": "tsx --env-file .env ./test/status.test.ts'",
		"test:status": "mocha -r ts-node/register -r tsconfig-paths/register ./test/status.test.ts",
		"fix": "biome fix .",
		"prepare": "husky"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@quikcess/bet-api-types": "^0.0.8",
		"ws": "^8.18.0",
		"zod": "^3.23.8"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/quikcess/sdk-bet.git"
	},
	"keywords": ["quikcess", "api", "bet", "bet api", "sdk"],
	"author": "Eydrenn Araujo",
	"bugs": {
		"url": "https://github.com/quikcess/sdk-bet/issues"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.6.4",
		"@changesets/cli": "^2.27.1",
		"@types/node": "^20.17.14",
		"@types/ws": "^8.5.13",
		"esm": "^3.2.25",
		"husky": "^9.0.11",
		"mocha": "^11.0.1",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsconfig-paths": "^4.2.0",
		"tsup": "^8.0.2",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	}
}
